pipeline{
    agent any
    environnment{
        PATH "C:/Users/sreen/Downloads/apache-maven-3.8.3-bin/apache-maven-3.8.3/bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'Git-Credentials', url: 'https://github.com/sreemachu/simple-java-project.git'
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"
            }
        }
    }
}
